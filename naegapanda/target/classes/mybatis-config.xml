<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.panda.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.panda.myShopping.ggim.model.vo.Ggim" alias="Ggim"/>
		<typeAlias type="com.kh.panda.myShopping.basket.model.vo.Basket" alias="Basket"/>
