		<typeAlias type="com.kh.panda.admin.violate.model.vo.Violate" alias="Violate"/>
	</typeAliases>

	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/ggim-mapper.xml"/>
		<mapper resource="mappers/basket-mapper.xml"/>
		<mapper resource="mappers/inquiry-mapper.xml"/>
