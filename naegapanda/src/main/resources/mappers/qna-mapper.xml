<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qnaMapper">
	
	<resultMap id="qnaResultSet" type="qna">
		<id property="qId" column="q_id" />
		<result property="qTitle" column="q_title" />
		<result property="qContent" column="q_content" />
		<result property="mNo" column="m_no" />
		<result property="sNo" column="s_no" />
		<result property="qCount" column="q_count" />
		<result property="qCreateDate" column="q_create_date" />
		<result property="qModifyDate" column="q_modify_date" />
		<result property="qStatus" column="q_status" />
	</resultMap>

	<select id="getListCount" resultType="_int">
		select count(*)
		from qna
		where q_status='N'
	</select>
	
	<select id="selectList" resultMap="qnaResultSet">
		select * 
		from qlist
	</select>
</mapper>